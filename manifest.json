{
  "name": "Anti NBAVote",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Hides #NBAVote posts and status updates from Facebook News Feed",
  "homepage_url": "http://stichoza.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://www.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}